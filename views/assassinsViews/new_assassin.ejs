

<form action="/assassins/posted" method = "post">
        <div class="form-group">
          <label for="full_name" >Full Name</label>
          <input name = "full_name" type="text" class="form-control" id="full_name" placeholder="Enter Name">
        </div>
        
        <div class="form-group">
          <label for="code_names">Code Names</label>
          <input name = "code_names" type="text" class="form-control" id="code_names" placeholder="Enter Code Names">
        </div>

        <div class="form-group">
            <label for="weapon">Weapon</label>
            <input name = "weapon" type="text" class="form-control" id="weapon" placeholder="Enter Weapon">
        </div>

        <div class="form-group">
            <label for="contact_info" class="col-sm-2 col-form-label col-form-label-lg">Contact Info</label>
            <input name= "contact_info" type="text" class="form-control" id="contact_info" placeholder="Enter Contact Info">
        </div>

        <div class="form-group">
            <label for="age" class="col-sm-2 col-form-label col-form-label-lg">Age</label>
            <input name = "age" type="text" class="form-control" id="newAssassinAge" placeholder="Enter Age">
        </div>

        <div class="form-group">
            <label for="price" class="col-sm-2 col-form-label col-form-label-lg">Price</label>
            <input name="price" type="text" class="form-control" id="newAssassinPrice" placeholder="Enter Price">
        </div>   
        
        <div class="form-group">
            <label for="rating" class="col-sm-2 col-form-label col-form-label-lg">Rating</label>
            <input name= "rating" type="text" class="form-control" id="newAssassinRating" placeholder="Enter Rating">
        </div>

        <div class="form-group">
            <label for = "kills" >Kills</label>
            <input name = "kills" type="text" class="form-control" id="newAssassinKills" placeholder="Enter No. of Kills">
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
      </form>